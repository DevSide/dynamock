{
  "name": "dynamock",
  "version": "1.2.2",
  "description": "Dynamic mock server which serves fixtures based on property matching",
  "repository": "https://github.com/DevSide/dynamock",
  "keywords": [
    "fixtures",
    "mock",
    "server",
    "e2e",
    "end-to-end",
    "functional testing"
  ],
  "author": "DevSide",
  "license": "MIT",
  "bin": "bin/dynamock.js",
  "main": "src/createServer.js",
  "scripts": {
    "check-git": "git status -uno --porcelain && [ -z \"$(git status -uno --porcelain)\" ] || (echo 'Git working directory not clean'; false)",
    "format": "npx @biomejs/biome format --write src",
    "lint": "npx @biomejs/biome lint --write src",
    "release": "semantic-release",
    "test": "jest src"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "body-parser": "1.20.2",
    "cookie-parser": "1.4.6",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "coveralls": "^3.1.1",
    "jest": "^26.2.2",
    "metro-memory-fs": "^0.78.1",
    "semantic-release": "^17.4.4",
    "supertest": "^6.3.3"
  }
}
