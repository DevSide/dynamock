{
  "name": "dynamock",
  "version": "2.1.0",
  "description": "Dynamic mock server which serves fixtures based on property matching",
  "repository": "https://github.com/DevSide/dynamock",
  "keywords": [
    "fixtures",
    "mock",
    "server",
    "e2e",
    "end-to-end",
    "functional testing"
  ],
  "author": "DevSide",
  "license": "MIT",
  "type": "module",
  "bin": "bin/dynamock.js",
  "main": "src/createServer.js",
  "scripts": {
    "check-git": "git status -uno --porcelain && [ -z \"$(git status -uno --porcelain)\" ] || (echo 'Git working directory not clean'; false)",
    "format": "npx @biomejs/biome format --write src",
    "lint": "npx @biomejs/biome lint --write src",
    "release": "semantic-release",
    "test": "NODE_OPTIONS='--enable-source-maps --experimental-vm-modules' jest src --coverage",
    "test:bin": "node test/bin.js"
  },
  "engines": {
    "node": ">=20"
  },
  "jest": {
    "transform": {}
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "cookie-parser": "1.4.7",
    "express": "4.21.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@jest/globals": "^29.7.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "coveralls": "^3.1.1",
    "jest": "^29.7.0",
    "metro-memory-fs": "^0.81.0",
    "semantic-release": "^24.1.2",
    "supertest": "^7.0.0"
  },
  "packageManager": "yarn@4.5.0"
}
